{
  "name": "nuxt-app",
  "version": "0.3.3",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "repository": "github:steadygaze/frame-randomizer",
  "scripts": {
    "build": "nuxt build",
    "check": "npm run format && npm run lint && npm run vue-tsc",
    "cm": "cz",
    "coverage": "vitest run --coverage.enabled --coverage.reporter=text --coverage.reporter=lcov",
    "dev": "nuxt dev",
    "format": "prettier --write --ignore-unknown .",
    "formatcheck": "prettier --check --ignore-unknown .",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "postinstall": "nuxt prepare",
    "precommit": "npm run formatcheck && npm run lint && npm run vue-tsc && npm run test && nuxt build pcdist",
    "prepare": "husky",
    "preview": "nuxt preview",
    "test": "vitest run",
    "testw": "vitest watch",
    "tsc": "tsc --noEmit",
    "vue-tsc": "vue-tsc"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxt/devtools": "^1.3.9",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/lodash.difference": "^4.5.9",
    "@types/lodash.intersection": "^4.4.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.once": "^4.1.9",
    "@types/shell-escape": "^0.2.3",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^1.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.12.3",
    "prettier": "^3.2.5",
    "prettier-plugin-sort-json": "^3.1.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.26"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^8.3.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.1",
    "@pinia/nuxt": "^0.5.1",
    "chalk": "^5.3.0",
    "detect-browser": "^5.3.0",
    "fuse.js": "^7.0.0",
    "glob": "^10.3.10",
    "grapheme-splitter": "^1.0.4",
    "json-stringify-deterministic": "^1.0.12",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "lodash.once": "^4.1.1",
    "nuxt-security": "^1.4.3",
    "p-limit": "^5.0.0",
    "pinia": "^2.1.7",
    "sass": "^1.72.0",
    "shell-escape": "^0.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown ."
    ],
    "*.test.{js,ts}": "vitest run",
    "*.{js,ts}": "eslint --fix"
  },
  "overrides": {
    "vue": "^3.2.47"
  }
}
